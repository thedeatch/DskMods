textures/xg/xglogo
{
    {
        map $lightmap
        rgbGen identity
        tcGen lightmap
    }
    {
        map textures/xg/xglogo.tga
        blendFunc GL_DST_COLOR GL_ZERO
        rgbGen identity
    }
    {
        map textures/xg/xglogo_glow.tga
        blendFunc GL_ONE GL_ONE
    }
}

